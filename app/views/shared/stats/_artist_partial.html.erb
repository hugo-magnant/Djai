  <% i = 1 %>
<% artists.each do |artist| %>
  <a href="<%= artist.external_urls["spotify"] %>" target="_blank" class="group">
    <div class="aspect-w-1 aspect-h-1 w-full overflow-hidden">
      <img src="<%= artist.images.first['url'] %>" class="h-full w-full object-cover object-center group-hover:opacity-75">
    </div>
    <p class="mt-4 text-center text-sm font-medium text-knoiz-blanc"><%= i %># - <%= artist.name %></p>
  </a>
  <% i = i + 1 %>
<% end %>
